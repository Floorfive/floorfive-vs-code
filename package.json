{
    "name": "floorfive-vs-code",
    "displayName": "FloorFive VS Code",
    "description": "Errevi developers extension with QoL functions",
    "version": "0.0.1",
    "publisher": "Errevi System srl",
    "engines": {
        "vscode": "^1.81.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "icon": "assets/logo/FloorFive.png",
    "contributes": {
        "commands": [
            {
                "command": "floorfive-vs-code.helloWorld",
                "title": "Hello World"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "floorfive-vs-code",
                    "title": "FloorFive",
                    "icon": "assets/logo/FloorFive_activitybar.png"
                }
            ],
            "panel": [
                {
                    "id": "floorfive-vs-code-panel",
                    "title": "FloorFive",
                    "icon": "assets/logo/FloorFive_activitybar.png"
                }
            ]
        },
        "views": {
            "floorfive-vs-code": [
                {
                    "id": "floorfive-vs-code.webview",
                    "name": "",
                    "icon": "assets/icons/code.svg",
                    "type": "webview"
                }
            ],
            "floorfive-vs-code-panel": [
                {
                    "id": "floorfive-vs-code-panel.webview",
                    "name": "",
                    "icon": "assets/icons/code.svg",
                    "type": "webview"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "lint-fix": "eslint src --ext ts --fix",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.81.0",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "@vscode/test-electron": "^2.3.4",
        "eslint": "^8.47.0",
        "glob": "^10.3.3",
        "mocha": "^10.2.0",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "highlight.js": "^11.8.0"
    }
}
