{
    "name": "FloorFive-VS-Code Documentation",
    "out": "docs", // The destination folder for documentation
    "entryPoints": ["./src"], // Paths of source files to be documented
    "entryPointStrategy": "expand",
    "exclude": ["**/node_modules/**", "**/*.spec.ts"], // Files or folders to exclude
    "includes": "*",
    "theme": "default", // Documentation theme (you can customize it)
    "tsconfig": "tsconfig.json", // Path to the tsconfig.json file
}
